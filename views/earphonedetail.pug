extends layout 
 
block content 
  h1= title 
 
  - let earphoneid = toShow._id 
  - let Brand = toShow.Brand
  - let earphone_type = toShow.earphone_type 
  - let Cost = toShow.Cost
  div 
    div.row 
      div(class='earphoneAttr mr-3') earphone ID is: 
      div #{earphoneid} 
    div.row 
      div(class='earphoneAttr mr-3') earphone Brand is: 
      div #{Brand} 
    div.row 
      div(class='earphoneAttr mr-3') earphone_Type is: 
      div #{earphone_type} 
    div.row 
      div(class='earphoneAttr mr-3') earphone Cost is: 
      div #{Cost} 